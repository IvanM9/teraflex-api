{
  "$schema": "https://json.schemastore.org/swcrc",
  "sourceMaps": true,
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "dynamicImport": true
    },
    "baseUrl": "src",
    "paths": {
          "@/*": ["*"],
          "@security/*": ["security/*"],
          "@activity/*": ["activities/*"],
          "@entities/*": ["entities/*"],
          "@multimedia/*": ["multimedia/*"],
          "@shared/*": ["shared/*"],
          "@user/*": ["user/*"],
        }
  },
  "minify": false,
  "env":{
  "dynamicImport": true,
  }
}
