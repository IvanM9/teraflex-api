{
  "$schema": "https://json.schemastore.org/swcrc",
  "sourceMaps": true,
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "dynamicImport": true
    },
    "baseUrl": "src",
    "paths": {
          "@/*": ["*"],
          "@security/*": ["security/*"],
          "@activities/*": ["activities/*"],
          "@entities/*": ["entities/*"],
          "@multimedia/*": ["multimedia/*"],
          "@shared/*": ["shared/*"],
          "@users/*": ["users/*"],
          "@notifications/*": ["notifications/*"],
        }
  },
  "minify": false
}
